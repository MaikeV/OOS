<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prak5client.LoginController">
   <children>
      <Label layoutX="58.0" layoutY="55.0" text="Benutzerverwaltung">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="58.0" layoutY="142.0" text="UserId:" />
      <Label layoutX="58.0" layoutY="200.0" text="Passwort:" />
      <TextField fx:id="tf_userId" layoutX="167.0" layoutY="137.0" prefHeight="26.0" prefWidth="279.0" />
      <TextField fx:id="tf_password" layoutX="167.0" layoutY="195.0" prefHeight="26.0" prefWidth="279.0" />
      <CheckBox fx:id="cb_new" layoutX="58.0" layoutY="262.0" mnemonicParsing="false" onAction="#checkbox" text="Neu Anmeldung" />
      <Button fx:id="btnLogin" layoutX="59.0" layoutY="321.0" mnemonicParsing="false" onAction="#login" text="Ausfuehren" />
      <Label fx:id="lbl_error" layoutX="169.0" layoutY="233.0" textFill="RED" />
      <CheckBox fx:id="cb_remote" layoutX="262.0" layoutY="262.0" mnemonicParsing="false" text="Remote Anmeldung" />
   </children>
</AnchorPane>
